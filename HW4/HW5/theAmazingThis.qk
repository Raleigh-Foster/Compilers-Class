


class Foo(){



   this.x = 40;
   def bar(x : Int) : Int{
     return x * 10;   
       
   }    
    
   def baz(x : Int) : Int{
       return this.biz().bar(this.x) + x;
   }
   
   def biz() : Foo{
    s = """Yes, this code gets executed correctly, before returning the this pointer!\n""";   
    s.PRINT();
    return this;
   }

}




Foo().baz(3).PRINT();


