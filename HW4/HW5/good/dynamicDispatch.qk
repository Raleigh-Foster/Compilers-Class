

// Dynamic dispatch WORKS, and also handles the *this* reference correctly.

class Parent(){
  this.x = 10;
  def foo() : Int{
   return this.bar();
  }    
  def bar() : Int{
    return (this.x)*3;  // careful! without parens, we're trying to index x*3 in this.... or something terrible like that.
  }
    
}


class Child() extends Parent{
  this.x = 5;
  def bar() : Int{
   return this.x;
  }
    
}


x = Parent();
x = Child();


x.foo().PRINT();

