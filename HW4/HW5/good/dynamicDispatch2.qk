






// This is a simple example of dynamic dispatch (static and dynamic type are different). It works.



class Red() extends Color{

    s = "calling red constructor";
    s.PRINT();

     def getRedness() : Int{
         return 255;    
     }



}





class Color() {
 
  s = "calling color constructor";
  s.PRINT();
  def getRedness() : Int{
      return 100;
  }



}


x = Red();

x.getRedness().PRINT();


x = Color();

x.getRedness().PRINT();

